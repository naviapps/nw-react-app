{
  "name": "nw-react-boilerplate",
  "version": "0.0.7",
  "description": "NW.js + React.js Boilerplate",
  "keywords": [
    "nw",
    "nw.js",
    "nwjs",
    "node-webkit",
    "react",
    "boilerplate"
  ],
  "bugs": {
    "url": "https://github.com/naviapps/nw-react-boilerplate/issues"
  },
  "license": "MIT",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/naviapps/nw-react-boilerplate.git"
  },
  "scripts": {
    "start": "webpack-dev-server --env=dev --progress --profile --colors",
    "prebuild": "rimraf dist",
    "nw": "run .",
    "build": "webpack --env=prod --progress --profile --colors; build --tasks win-x86,win-x64,mac-x64 .",
    "lint": "eslint src; exit 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redbox-react": "^1.4.3",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.51.1",
    "nwjs-builder-phoenix": "^1.14.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "window": {
    "width": 800,
    "height": 500
  },
  "build": {
    "nwVersion": "0.24.0",
    "files": [
      "./index.html",
      "./dist/bundle.js"
    ]
  }
}
